{
  "name": "tutorial-editor",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://netlogo-mobile.github.io/Tutorial-Editor",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@rjsf/core": "^2.2.2",
    "@rjsf/material-ui": "^2.2.1",
    "@rjsf/semantic-ui": "^2.2.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "json-rules-engine-simplified": "^0.1.17",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jsonschema-form": "^1.8.1",
    "react-scripts": "3.4.1",
    "rjsf-conditionals": "^1.0.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --fix",
    "format": "prettier --trailing-comma all --single-quote --write 'src/*' 'public/*.json'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json,css,html}": [
      "prettier --trailing-comma all --single-quote --write",
      "git add"
    ]
  }
}
